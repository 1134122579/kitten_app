.couponStyle{
  margin: 20rpx;
  border-radius: 8rpx;
   box-shadow: 0 0 8rpx #cccccc;
  position: relative;
  box-sizing: border-box;
  height: 380rpx;
  overflow: hidden;
}

.couponStyle_l{
  /* width: 100px;
  height: 120px; */
  border-radius: 5rpx;
  overflow: hidden;
}
.codeImg{
  width: 100%;
  height: 100%;
}
.couponStyle_content_z{

  position: absolute;
  top: 0%;
  width: 100%;
  height: 100%;
}
.couponStyle_content_z_header{
  display: flex;
  justify-content: space-between;
  align-items:center;
  height: 50%;box-sizing: border-box;
