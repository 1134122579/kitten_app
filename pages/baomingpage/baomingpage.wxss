page {
  width: 100%;
  box-sizing: border-box;
  background: #fafafa;
  font-size: 28rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  /*兼容 IOS<11.2*/
  padding-bottom: env(safe-area-inset-bottom);
  /*兼容 IOS>11.2*/
}

.lableStyle {
  padding: 20rpx;
  background: #fff;
}

.lable_value {
  color: #dfd9d9;
}

/* 猫咪列表 */
.catListPage {
  padding: 20rpx;
  box-sizing: border-box;
  padding-bottom: 120px;
}


/* 按钮 */
.button_ {
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  padding: 20rpx;
  padding-bottom: 40rpx;
  box-sizing: border-box;
  background: rgb(255, 255, 255);

}