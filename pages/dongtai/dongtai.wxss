/* pages/enrollpage/enrollpage.wxss */
page{
  --cell-large-vertical-padding:40rpx;
  background-color: rgba(232, 232, 232, 1);
}
.cat_detail{
  margin-bottom: 20rpx;
  background: #fff;
} 
.e_title{
  font-size: 16px;
  color: #333;
  font-weight: 600;
  padding: 14px 10px;
  letter-spacing: 2px;
}
.inputStyle{
  padding: 10px 0;
}
.e_block .van-field {
  padding: 10px 0;
}
.labelClass{
  width: 120px;
}
.uplodaStyle{
  padding:0 10px;
}
.button_{
  padding: 20px;
  background: #fff;
  position: fixed;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
}
.labelStyle{
  padding: 20rpx;
}
.label{
padding:5rpx 20rpx ;
font-size: 24rpx;
background-color: rgb(212, 211, 211);
border-radius: 30rpx;
}
.dreass{
  padding: 18rpx;
  
}
.lable_title{
  margin-left: 10rpx;
  font-size: 24rpx;
}

/* 标签 */
.poupList{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 100%;
  height: 100%;
}
.listStyle{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
  flex: 1;
  overflow-y: auto;
}
.listname{
  border: 1px solid #e7e4e4;
  padding: 10rpx 20rpx;
  margin: 10rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
}
.suer{
  border: 1px solid var(--button-warning-background-color);
  color: var(--button-warning-background-color);
}
.button_list{
    background:var(--button-warning-background-color) ;
    width: 80%;
    margin: 20rpx auto;
    flex-shrink: 0;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #fff;
    border-radius: 40rpx;
}